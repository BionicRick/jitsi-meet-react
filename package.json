{
  "name": "jitsi-meet-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build:web": "mkdir -p dist/web; cp -r static/* dist/web; cp -r web/static/* dist/web; webpack --config web/webpack.config.js",
    "build:desktop": "mkdir -p dist/desktop; cp -r static/* dist/desktop; cp -r desktop/static/* dist/desktop; webpack --config desktop/webpack.config.js",
    "start:ios": "npm run clean; react-native run-ios",
    "start:web": "webpack-dev-server --config web/webpack.config.js --colors --port 5000 --https --history-api-fallback --open",
    "start:desktop": "npm run start:electron",
    "start:electron": "npm run build:desktop && electron dist/desktop/electron.js",
    "start:nwjs": "npm run build:desktop && /Applications/nwjs.app/Contents/MacOS/nwjs dist/desktop"
  },
  "dependencies": {
    "electron-prebuilt": "^1.2.1",
    "html-webpack-plugin": "^2.19.0",
    "jitsi-meet-logger": "jitsi/jitsi-meet-logger",
    "jquery": "~2.1.1",
    "lib-jitsi-meet": "jitsi/lib-jitsi-meet",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-native": "0.27.0",
    "react-native-webrtc": "lyubomir/react-native-webrtc#lib-jitsi-meet",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "strophe": "^1.2.2",
    "strophejs-plugins": "^0.0.6",
    "xmldom": "0.1.22"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "browserify": "11.1.x",
    "exorcist": "*",
    "uglify-js": "2.4.24"
  }
}
